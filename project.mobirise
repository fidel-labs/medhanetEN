          "_JSplaceholders": [
            "<script>\n    function goToSearchWithValue() {\n        const input = document.getElementById(\"search-input\");\n        const searchButton = document.getElementsByClassName(\"search-icon\");\n        searchButton[0].href = 'https://app.medhan.et/find-drug?q='+input.value;\n        \n        console.log('Called', searchButton)\n        const url = new URL(window.location.href);\n        const params = url.searchParams;\n        \n        console.log(window.location.href);\n        const utm_source = params.get('utm_source');\n        if(utm_source) {\n            searchButton.href = searchButton.href + '&utm_source='+utm_source;\n        }\n        const utm_medium = params.get('utm_medium');\n        if(utm_medium) {\n            searchButton.href = searchButton.href + '&utm_medium='+utm_medium;\n        }\n        const utm_campaign = params.get('utm_campaign');\n        if(utm_campaign) {\n            searchButton.href = searchButton.href + '&utm_campaign='+utm_campaign;\n        }\n        \n    }\n</script>"
